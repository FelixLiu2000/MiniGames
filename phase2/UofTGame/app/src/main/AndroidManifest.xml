<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.group0611.uoftgame">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.RemovePlayerActivity"></activity>
        <activity android:name=".activities.ResultsPageActivity" />
        <activity android:name=".activities.SettingsActivity" />
        <activity android:name=".activities.SubwayGameActivity" />
        <activity android:name=".activities.LogInActivity" />
        <activity android:name=".activities.CreateUserActivity" />
        <activity android:name=".activities.BallGameActivity" />
        <activity android:name=".activities.CardGameActivity" />
        <activity android:name=".activities.GameDashboardActivity" />
        <activity android:name=".activities.AppStartActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>